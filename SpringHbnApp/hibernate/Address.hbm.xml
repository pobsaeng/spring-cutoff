<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<hibernate-mapping>
	   <class name="com.app.model.Address">
		 <id name="adrId" column="adrId">
		 	<generator class="identity" />
		 </id>
	
		<property name="adrHomeNum" length="50" not-null = "false" column = "adrHomeNum" />
		<property name="adrRoad" length="50" not-null = "false" column = "adrRoad" />
		<property name="adrProvince" length="50" not-null = "false" column = "adrProvince" />
		<property name="adrZipCode" length="25" not-null = "false" column = "adrZipCode" />

<!-- 		<many-to-one name="customer" column="cusId" class="com.app.model.Customer"/> -->
		
	   </class>
	</hibernate-mapping>
